<aside class="span8 liked-shops">
	<h2><%= @user.name %>'s Liked Shops </h2>
	<table>
		<thead>
			<tr>
				<th>Coffee Shop</th>
				<th>Address</th>
			</tr>
		</thead>

		<tbody>
		<% @shops.each do |shop| %>
			<tr>
				<td class="name">
					<%= shop.name %>
				</td>
				<td class="address"> 
					<%= shop.address %>
				</td>
			</tr>
	<% end %>
		</tbody>
	</table>
</aside>
